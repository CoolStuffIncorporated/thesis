    "node": "10.5.0"
  },
  "config": {
    "ghooks": {
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "jest": {
    "setupFiles": ["./src/tests/setupTests.js"],
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  }
}
