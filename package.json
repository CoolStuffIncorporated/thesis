{
  "name": "thesis",
  "version": "1.0.0",
  "description": "Find your adventure!",
  "main": "index.js",
  "scripts": {
    "build-watch": "webpack -w -d",
    "start-watch": "nodemon server/index.js",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "build": "webpack",
    "start": "npm run build && node server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "selenium-setup": "selenium-standalone install",
    "selenium-start": "selenium-standalone start",
    "e2e-tests": "wdio wdio.conf.js",
    "e2e-tests-watch": "wdio wdio.conf.js --watch",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoolStuffIncorporated/thesis.git"
  },
  "author": "Charles Hufnagel, Koichi Sakamaki, Lina Lei, Mandy Mak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CoolStuffIncorporated/thesis/issues"
  },
  "homepage": "https://github.com/CoolStuffIncorporated/thesis#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "ghooks": "^2.0.4",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.5",
    "react-test-renderer": "^16.4.1",
    "selenium-standalone": "^6.15.1",
    "wdio-mocha-framework": "^0.5.13",
    "webdriverio": "^4.13.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "commitizen": "^2.10.1",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.3",
    "fetch-mock": "^6.4.4",
    "install": "^0.12.0",
